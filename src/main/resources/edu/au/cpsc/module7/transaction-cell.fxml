   <children>
      <VBox fx:id="transactionListCell" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label fx:id="transactionCellItemLabel" text="Label" />
            <Label fx:id="transactionCellAmountLabel" text="Label" />
         </children>
      </VBox>
      <HBox alignment="CENTER_RIGHT" prefHeight="42.0" prefWidth="161.0">
         <children>
            <Button fx:id="transactionCellEditButton" minWidth="60.0" mnemonicParsing="false" text="Edit" />
